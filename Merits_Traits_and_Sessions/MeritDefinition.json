{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "meritDataType": {
            "@id": "[MeritDataType](#MeritDataType)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Data Type of the merit"
        },
        "meritFormula": {
            "@id": "[MeritFormula](#MeritFormula)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Formual used to compute the merit"
        },
        "meritName": {
            "@id": "[MeritName](#MeritName)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Name of Merit type in URN string format"
        },
        "meritShortName": {
            "@id": "[MeritShortName](#MeritShortName)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Short name of Merit type in URN string format"
        },
        "meritType": {
            "@id": "[MeritType](#MeritType)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Indicates what type of merit it is within the provider system. Eg, In SIL it can Breeding value or Index, In LIC it can be BW or PW"
        },
        "meritUnit": {
            "@id": "[MeritUnit](#MeritUnit)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unit of measurement for the merit"
        },
        "valueFormat":{
            "@id": "[ValueFormat](#ValueFormat)",
            "@type": "xsd: string",
            "type": "string",
            "dldescription": "Description of how to format the merit values for displaying"
        }
    }
