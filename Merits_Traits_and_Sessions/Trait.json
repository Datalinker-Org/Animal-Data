{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "animalUrl": {
            "@id": "[AnimalUrl](#AnimalUrl)",
            "@type": "@id",
            "type": "string",
            "format": "uri",
            "dldescription": "URL provides further information on the animal",
            "dltype": "[Animal](Animal.md)"
        },
        "traitDefinition": {
            "@id": "[TraitDefinition](#TraitDefinition)",
            "@type": "@id",
            "type": "string",
            "format": "uri",
            "dldescription": "Definitions valid for the herd",
            "dltype": "[TraitDefinition](TraitDefinition.md)"
        },
        "recordingHerd": {
            "@id": "[RecordingHerd](#RecordingHerd)",
            "@type": "@id",
            "type": "string",
            "format": "uri",
            "dldescription": "URL provides herd information for which the trait was recorded",
            "dltype": "[Herd](Herd.md)"
        },
        "animalIdentifiers": {
            "@id": "[AnimalIdentifiers](AnimalIdentifiers.md)",
            "@type": "@id",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "Array of animal identifiers"
        },
        "recordingYear": {
            "@id": "[RecordingYear](#RecordingYear)",
            "@type": "xsd:integer",
            "type": "string",
            "dldescription": "Year in which the trait was recorded"
        },
        "recordingPeriod": {
            "@id": "[RecordingPeriod](#RecordingPeriod)",
            "@type": "xsd:integer",
            "type": "string",
            "dldescription": "Month of the year in which the trait was recorded"
        },
        "recordingMob": {
            "@id": "[RecordingMob](#RecordingMob)",
            "@type": "xsd:integer",
            "type": "string",
            "dldescription": "Mob number usually 0-9"
        },
        "recordingDate": {
            "@id": "[RecordingDate](#RecordingDate)",
            "@type": "xsd:date",
            "type": "string",
            "dldescription": "Date of recording of the trait"
        },
        "traitCode": {
            "@id": "[TraitCode](#TraitCode)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "URN based short code that represents the trait"
        },
        "traitValue": {
            "@id": "[TraitValue](#TraitValue)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Observed value of the trait"
        }
    }
}
