{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "organization": {
      "@id": "[Organization](https://github.com/Datalinker-Org/Shared/blob/master/Organization.md)",
      "@type": "@id",
      "type": "object",
      "dldescription": "The official name or the registered name of the organization"
    },
    "sessionDate": {
      "@id": "[SessionDate](#SessionDate)",
      "@type": "xsd:date",
      "type": "string",
      "dldescription": "Date on which a session of observations were recorded"
    },
    "sessionName": {
      "@id": "[SessionName](#SessionName)",
      "@type": "xsd:date",
      "type": "string",
      "dldescription": "Name of the recorded session"
    },
    "reference": {
      "@id": "[SessionReference](#SessionReference)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Indicates the session reference"
    },
    "herdCodes": {
      "@id": "[Herd](#Herd)",
      "@type": "xsd:string",
      "type": "array",
      "items": { "type": "string" },
      "@container": "@set",
      "dldescription": "Array of herds"
    },
    "sessionObservations": {
      "@id": "[SessionObservations](SessionObservations.md)",
      "@type": "@id",
      "type": "array",
      "items": { "type": "object" },
      "@container": "@set",
      "dldescription": "Array of Observations"
    },
    "sessionAnimals": {
      "@id": "[SessionAnimals](SessionAnimals.md)",
      "@type": "@id",
      "type": "array",
      "items": { "type": "object" },
      "@container": "@set",
      "dldescription": "Array of Animals"
    }
  }
}
