{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "traitCode": {
            "@id": "[TraitCode](#TraitCode)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Namespace that represents the trait.  A suggestion is <site><identifier><trait><trait code> eg:"
        },
        "traitType": {
            "@id": "[TraitType](#TraitType)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Usually free text which indicates what kind of trait it is"
        },
        "traitName": {
            "@id": "[TraitName](#TraitName)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Name of the trait"
        },
        "traitDataType": {
            "@id": "[TraitDataType](#TraitDataType)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Indicates how the trait data can be represented"
        },
        "traitUnits": {
            "@id": "[TraitUnits](#TraitUnits)",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unit in which the trait is recorded"
        }
    }
}
