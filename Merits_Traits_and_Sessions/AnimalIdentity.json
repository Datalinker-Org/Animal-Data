{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "animalIdentifiers": {
            "@id": "https://www.datalinker.org/context/AnimalIdentifier",
            "@type": "@id",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "Provides reference to the identifier used to represent the animal at various states - birth, current or historic"
        },
        "herdState": {
            "@id": "https://www.datalinker.org/context/AnimalHerdState",
            "@type": "@id",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "Provides reference to the herd the animal belongs to various states - birth, current or historic"
        },
        "locations": {
            "@id": "https://www.datalinker.org/context/AnimalLocationState",
            "@type": "@id",
            "type": "array",
            "items": {
                "type": "object"
            },
            "@container": "@set",
            "dldescription": "Provides reference to the location the animal has been at various states - birth, current or historic  "
        }
    }
}
