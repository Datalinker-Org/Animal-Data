{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "@id": "https://www.datalinker.org/context/DairyHolding-Id",
      "@type": "@id",
      "type": "string",
      "format": "uri",
      "dldescription": "Unique identifier in the source system"
    },
    "identifiers": {
      "@id": "https://www.datalinker.org/context/DairyHolding-Identifiers",
      "@type": "xsd:string",
      "type": "string",
      "@contaner": "@set",
      "dldescription": "Array of unique URN identifiers"
    },
    "name": {
      "@id": "https://www.datalinker.org/context/DairyHolding-Name",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Display name for the holding"
    },
    "dateModified": {
      "@id": "http://schema.org/dateModified",
      "@type": "xsd:dateTime",
      "type": "string",
      "dldescription": "Date when details were last modified"
    }
  }
}