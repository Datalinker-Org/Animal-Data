{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "@id": "[id](#id)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Unique Id of the animal event"
    },
    "animalId": {
      "@id": "[animalId](#animalId)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Unique Id of the related animal"
    },
    "herdEventId": {
      "@id": "[herdEventId](#herdEventId)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Unique Id of the related herd event"
    },
    "animalEarNumber": {
      "@id": "[animalEarNumber](#animalEarNumber)",
      "@type": "xsd:integer",
      "type": "string",
      "dldescription": "The ear number of the inseminated cow"
    },
    "batchNumber": {
      "@id": "[batchNumber](#batchNumber)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Batch or Freezing Number"
    },
    "containerId": {
      "@id": "[containerId](#containerId)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Farm's container number from where the bull dose was taken. NULL for natural matings"
    },
    "doseUsage": {
      "@id": "[doseUsage](#doseUsage)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Dose usage - see codes. NULL for natural matings (1 dose used, 2 dose used, last dose used (of batch))"
    },
    "heatStrength": {
      "@id": "[heatStrength](#heatStrength)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Observed sign/strength of heat (https://test.nordiccdx.com/Help/Codeset?name=HEAT_STRENGTH)"
    },
    "inseminationType": {
      "@id": "[inseminationType](#inseminationType)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "The type of the insemination (https://test.nordiccdx.com/Help/Codeset?name=INSEMINATION_TYPE)"
    },
    "inseminator": {
      "@id": "[inseminator](#inseminator)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Inseminator's id number, has to be a licensed person for artificial inseminations, NULL for natural matings"
    },
    "insemNumber": {
      "@id": "[insemNumber](#insemNumber)",
      "@type": "xsd:integer",
      "type": "string",
      "dldescription": "Serial number of insemination after the last calving"
    },
    "serviceSireBreed": {
      "@id": "[serviceSireBreed](#serviceSireBreed)",
      "@type": "xsd:integer",
      "type": "string",
      "dldescription": "The breed for the insemination sire (https://test.nordiccdx.com/Help/Codeset?name=BREED)"
    },
    "serviceSireHerdBookNumber": {
      "@id": "[serviceSireHerdBookNumber](#serviceSireHerdBookNumber)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "The herdbooknr for the insemination sire"
    },
    "serviceSireIndentifer": {
      "@id": "[serviceSireIndentifer](#serviceSireIndentifer)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "The animal identification of the bull"
    },
    "serviceSireName": {
      "@id": "[serviceSireName](#serviceSireName)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "The name for the insemination sire"
    },
    "typeOfSemen": {
      "@id": "[typeOfSemen](#typeOfSemen)",
      "@type": "xsd:integer",
      "type": "string",
      "dldescription": "The semen type (https://test.nordiccdx.com/Help/Codeset?name=SEMEN_TYPE)"
    }
  }
}
