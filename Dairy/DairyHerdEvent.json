{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "@id": "[id](#id)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Unique Id of the event"
    },
    "date": {
      "@id": "[date](#date)",
      "@type": "xsd:dateTime",
      "type": "string",
      "dldescription": "The date the event occured"
    },
    "identifiers": {
      "@id": "[id](#id)entifiers",
      "@container": "@set",
      "@type": "xsd:string",
      "type": "array",
      "items": { "type": "string" },
      "dldescription": "The list of globally unique animal identifiers invloved in the event"
    },
    "eventType": {
      "@id": "[eventType](#eventType)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "The type of Event this is (Reproduction, Milking, Movement, Weigh)"
    },
    "content": {
      "@id": "[content](#content)",
      "@type": "xsd:object",
      "type": "string",
      "dldescription": "The class specified by event type eg.(Reproduction, Milking, Movement, Weigh)"
    }
  }
}
