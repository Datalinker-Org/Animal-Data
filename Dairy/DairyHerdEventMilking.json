{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "@id": "[id](#id)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Unique Id of the herd milking event"
    },
    "dairyHerdEventId": {
      "@id": "[dairyHerdEventId](#dairyHerdEventId)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "herd event id that this milking event is part of"
    },
    "milkingMode": {
      "@id": "[milkingMode](#milkingMode)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "The method of milking (Manual or Robotic)"
    },
    "sampleBatchIds": {
      "@id": "[sampleBatchIds](#sampleBatchIds)",
      "@container": "@set",
      "@type": "xsd:string",
      "type": "array",
      "items": { "type": "string" },
      "dldescription": "The list of samples ids that were taken during the milking event"
    },
    "sampleMethod": {
      "@id": "[sampleMethod](#sampleMethod)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "The method of sampling"
    }
  }
}
